<%- include("templates/header") %>
<%- include("styles/admin_style") %>
<%- include("templates/header_closer") %>

    <h1> Welcome Admin <%= name %> </h1>

    <table>
        <tr>
            <th> Username </th>
            <th> Role </th>
        </tr>
        <% users.forEach(function(user) { %>
            <tr>
                <td> <%= user.username %> </td>
                <td> <%= user.role %> </td>
            </tr>
        <% }); %>
    </table>

